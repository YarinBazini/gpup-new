<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="main_screen" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="around_table" stylesheets="@../../resources/mainCssWorker.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.generalComponents.workerTasks.WorkerTasksController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="around_table">
         <content>
            <TableView id="table-view" fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <columns>
                  <TableColumn fx:id="nameCol" maxWidth="1.7976931348623157E308" minWidth="70.0" prefWidth="-1.0" text="Name" />
                  <TableColumn fx:id="statusCol" prefWidth="100.0" text="Task Status" />
                  <TableColumn fx:id="workersCol" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="-1.0" text="Workers" />
                  <TableColumn fx:id="targetsCol" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="-1.0" resizable="false" text="Targets" />
                  <TableColumn fx:id="doneTargetsCol" maxWidth="1.7976931348623157E308" minWidth="110.0" prefWidth="-1.0" resizable="false" text="Done Targets" />
                  <TableColumn fx:id="targetByMeCol" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="-1.0" resizable="false" text="Targets By Me" />
                  <TableColumn fx:id="creditCol" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Credit" />
                  <TableColumn fx:id="activeCol" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" resizable="false" text="Pause" />
               </columns>
            </TableView>
         </content>
      </ScrollPane>
   </children>
</StackPane>
