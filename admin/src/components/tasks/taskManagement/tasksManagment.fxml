                        <Insets bottom="10.0" top="10.0" />
                     </GridPane.margin>
                  </Label>
                  <VBox spacing="15.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Select task from the table" />
                        <StackPane fx:id="table_SP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
                  <Button fx:id="next_btn" maxHeight="1.7976931348623157E308" minWidth="100.0" mnemonicParsing="false" onAction="#ClickNext" text="Next" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="4" GridPane.vgrow="NEVER" />
