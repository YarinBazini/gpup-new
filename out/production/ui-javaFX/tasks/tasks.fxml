                                       </VBox.margin>
                                       <children>
                                          <Button fx:id="select_all_btn" maxHeight="1.7976931348623157E308" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#clickSelectAll" prefWidth="100.0" styleClass="deselect_btn" text="Select All" />
